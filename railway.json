{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "npm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "on_failure",
    "port": 8080
  },
  "env": {
    "NODE_ENV": "production",
    "RAILWAY_LOG_LEVEL": "debug",
    "PORT": "8080"
  }
}
