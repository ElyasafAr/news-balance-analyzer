{
  "build": {
    "builder": "RAILPACK",
    "watchPatterns": ["**/*"]
  },
  "deploy": {
    "startCommand": "npm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "on_failure"
  },
  "env": {
    "NODE_ENV": "production",
    "RAILWAY_LOG_LEVEL": "debug"
  }
}
